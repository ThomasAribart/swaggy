{
  "name": "swaggy",
  "description": "Share OpenAPI documentation effortlessly",
  "main": "index.js",
  "scripts": {
    "serve": "vite preview",
    "start": "vite",
    "test": "pnpm test-type && pnpm test-unit && pnpm test-circular && pnpm test-unused-exports && pnpm test-linter",
    "test-type": "tsc --noEmit",
    "test-unit": "vitest run --passWithNoTests",
    "test-unit-watch": "vitest --passWithNoTests",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js .",
    "test-unused-exports": "./node_modules/.bin/ts-unused-exports ./tsconfig.json --excludeDeclarationFiles --silent --excludePathsFromReport='vite.config.ts;'",
    "test-linter": "eslint --ext=js,jsx,ts,tsx src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.15",
    "@mui/styled-engine": "^5.15.14",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@types/swagger-ui-react": "^4.18.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dependency-cruiser": "^16.3.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-risxss": "^2.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jsdom": "^24.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.22.3",
    "swagger-ui-react": "^4.19.1",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.4.4",
    "vanilla-jsoneditor": "^0.23.1",
    "vite": "^5.2.8",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
